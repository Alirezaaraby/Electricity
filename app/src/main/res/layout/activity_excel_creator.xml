<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Excel_Creator"
    android:id="@+id/Parent_Coordinator_Excel">
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/scrollView">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:gravity="center"
        android:orientation="vertical"
        tools:context=".MainActivity">

        <TextView
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="@string/Header"
            android:textSize="25sp"/>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/SUBSTATION"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/SUBSTATION"
            android:layout_marginBottom="12dp"
            app:endIconMode="clear_text"
            app:startIconDrawable="@drawable/baseline_location_on_black_24dp"
            android:layout_marginTop="13dp"
            >
            <AutoCompleteTextView
                android:id="@+id/SUBSTATION______"
                style="@style/Widget.MaterialComponents.AutoCompleteTextView.FilledBox"
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:completionThreshold="1"
                android:singleLine="true"
                android:text="1"
                android:textCursorDrawable="@null" />


        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/BAY_NO"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/BAY_NO"
            app:endIconMode="clear_text"
            android:layout_marginBottom="12dp"
            >

            <AutoCompleteTextView
                android:id="@+id/BAY_NO______"
                style="@style/Widget.MaterialComponents.AutoCompleteTextView.FilledBox"
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:completionThreshold="1"
                android:singleLine="true"
                android:text="1"
                android:textCursorDrawable="@null" />

        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/PANEL_NO"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/PANEL_NO"
            app:endIconMode="clear_text"
            android:layout_marginBottom="12dp"
            >

            <AutoCompleteTextView
                android:id="@+id/PANEL_NO______"
                style="@style/Widget.MaterialComponents.AutoCompleteTextView.FilledBox"
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:completionThreshold="1"
                android:singleLine="true"
                android:text="1"
                android:textCursorDrawable="@null" />

        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/METER_MANUFACTURER"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/METER_MANUFACTURER"
            app:endIconMode="clear_text"
            android:layout_marginBottom="12dp"
            >

            <AutoCompleteTextView
                android:id="@+id/METER_MANUFACTURER______"
