                android:layout_height="60dp"
                android:singleLine="true"
                android:inputType="numberDecimal" android:text="1"
                android:id="@+id/REAL_VALUEI_LOAD_PH_C_"/>

        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="25sp"
            android:text="@string/I_ONLOAD_by_clamp"/>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/I_ONLOAD_by_clamp_PH_A"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/I_ONLOAD_by_clamp_PH_A"
            style="@style/TextInputLayoutStyle"
            app:endIconMode="clear_text"
            android:layout_marginBottom="12dp"
            android:layout_marginTop="13dp"
            >

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:singleLine="true"
                android:inputType="numberDecimal" android:text="1"
                android:id="@+id/I_ONLOAD_by_clamp_PH_A_"/>

        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/I_ONLOAD_by_clamp_PH_B"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/I_ONLOAD_by_clamp_PH_B"
