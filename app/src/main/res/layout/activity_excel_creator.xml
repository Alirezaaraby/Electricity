            android:layout_height="wrap_content"
            android:hint="@string/REALـVALUEـIـLOAD_PHASE_A"
            style="@style/TextInputLayoutStyle"
            app:endIconMode="clear_text"
            android:layout_marginBottom="12dp" android:layout_marginTop="20sp"
            >

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:singleLine="true"
                android:inputType="numberDecimal" android:text="1"
                android:id="@+id/REAL_VALUEI_LOAD_PHASE_A_"/>

        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/REAL_VALUEI_LOAD_PH_B"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/REALـVALUEـIـLOAD_PH_B"
            style="@style/TextInputLayoutStyle"
            app:endIconMode="clear_text"
            android:layout_marginBottom="12dp"
            >

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:singleLine="true"
                android:inputType="numberDecimal" android:text="1"
                android:id="@+id/REAL_VALUEI_LOAD_PH_B_"/>

        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
